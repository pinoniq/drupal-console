{
    "name": "pinoniq/drupal-console",
    "description": "A Drupal CLI. A tool to generate boilerplate code, interact with and debug Drupal.",
    "keywords": [
        "Drupal",
        "Console",
        "Development",
        "Symfony"
    ],
    "homepage": "https://github.com/pinoniq/drupal-console",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Drupal Console Contributors",
            "homepage": "https://github.com/pinoniq/drupal-console/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/pinoniq/drupal-console/issues"
    },
    "require": {
        "php": ">=7.4",
        "alchemy/zippy": "~0.4",
        "composer/installers": "~1.0",
        "doctrine/annotations": "^1.2",
        "doctrine/collections": "^1.3",
        "pinoniq/drupal-console-extend-plugin": "~1.0",
        "psy/psysh": "0.6.* || ~0.8",
        "symfony/css-selector": "~3.0|~4.0",
        "symfony/dom-crawler": "~3.0|~4.0",
        "symfony/http-foundation": "~3.0|~4.0",
        "dflydev/dot-access-configuration": "^1.0",
        "pinoniq/drupal-console-en": "~2.0",
        "stecman/symfony-console-completion": "~0.7",
        "symfony/config": "~3.0|^4.4",
        "symfony/console": "~3.0|^4.4",
        "symfony/debug": "~3.0|^4.4",
        "symfony/dependency-injection": "~3.0|^4.4",
        "symfony/event-dispatcher": "~3.0|^4.4",
        "symfony/filesystem": "~3.0|^4.4",
        "symfony/finder": "~3.0|^4.4",
        "symfony/process": "~3.0|^4.4",
        "symfony/translation": "~3.0|^4.4",
        "symfony/yaml": "~3.0|^4.4",
        "twig/twig": "^1.38.2|^2.12.0",
        "webflo/drupal-finder": "^1.0",
        "webmozart/path-util": "^2.3",
        "guzzlehttp/guzzle": "~6.1",
        "composer-plugin-api": "^1.0 || ^2.0"
    },
    "suggest": {
        "symfony/thanks": "Thank your favorite PHP projects on GitHub using the CLI",
        "vlucas/phpdotenv": "Loads environment variables from .env to getenv(), $_ENV and $_SERVER automagically"
    },
    "bin": [
        "bin/drupal"
    ],
    "config": {
        "bin-dir": "bin/",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Drupal\\Console\\": "src"
        }
    }
}